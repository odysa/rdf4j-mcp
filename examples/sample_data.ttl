@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .

# =============================================================================
# ONTOLOGY DEFINITION
# =============================================================================

# Classes
ex:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A human being" .

ex:Organization a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "A company, institution, or other organized body" .

ex:Project a owl:Class ;
    rdfs:label "Project" ;
    rdfs:comment "A planned endeavor with specific goals" .

ex:Technology a owl:Class ;
    rdfs:label "Technology" ;
    rdfs:comment "A tool, framework, or technical system" .

ex:Department a owl:Class ;
    rdfs:label "Department" ;
    rdfs:comment "A division within an organization" .

# Properties
ex:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain ex:Person ;
    rdfs:range xsd:string .

ex:email a owl:DatatypeProperty ;
    rdfs:label "email" ;
    rdfs:domain ex:Person ;
    rdfs:range xsd:string .

ex:worksFor a owl:ObjectProperty ;
    rdfs:label "works for" ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Organization .

ex:memberOf a owl:ObjectProperty ;
    rdfs:label "member of" ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Department .

ex:worksOn a owl:ObjectProperty ;
    rdfs:label "works on" ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Project .

ex:uses a owl:ObjectProperty ;
    rdfs:label "uses" ;
    rdfs:domain ex:Project ;
    rdfs:range ex:Technology .

ex:hasDepartment a owl:ObjectProperty ;
    rdfs:label "has department" ;
    rdfs:domain ex:Organization ;
    rdfs:range ex:Department .

ex:startDate a owl:DatatypeProperty ;
    rdfs:label "start date" ;
    rdfs:domain ex:Project ;
    rdfs:range xsd:date .

ex:budget a owl:DatatypeProperty ;
    rdfs:label "budget" ;
    rdfs:domain ex:Project ;
    rdfs:range xsd:decimal .

ex:status a owl:DatatypeProperty ;
    rdfs:label "status" ;
    rdfs:domain ex:Project ;
    rdfs:range xsd:string .

